    "@suites/types.doubles": "workspace:^3.0.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/spy": "^1.6.1",
    "vitest": "^1.6.1"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
